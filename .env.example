# Protocol Hive: Titan X - Environment Configuration
# Copy this file to .env and fill in your actual values

# Telegram Bot Configuration
TELEGRAM_TOKEN=your_bot_token_here

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/hive

# Admin Configuration
ADMIN_ID=your_telegram_user_id

# Security
POSTBACK_SECRET=your_secret_key_12345

# Monetization Links
OFFERTORO_LINK=https://www.offertoro.com/ifr/show/YOUR_APP_ID
ADSTERRA_LINK=https://www.adsterra.com/your_link_here

# Payment Configuration
CRYPTO_PAYMENT_ADDRESS=0xYourWalletAddressHere

# Server Configuration (Optional)
PORT=8000
